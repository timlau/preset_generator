{
  "plugins": [
    "generator.plugins.grid",
    "generator.plugins.slidein",
    "generator.plugins.pip"
  ],
  "generators": [
    {
      "type": "grid",
      "name": "Grid"
    },
    {
      "type": "slidein",
      "name": "SlideIn",
      "width": 3840,
      "height": 2150,
      "fps": 30,
      "duration": 5,
      "size": 50,
      "padding": 32
    },
    {
      "type": "pip",
      "name": "Pip",
      "width": 3840,
      "height": 2150,
      "size": 50,
      "padding": 32
    }
  ],
  "settings": {
    "output": "./Shotcut",
    "LIXUX_PATHS": [
      "~/.var/app/org.shotcut.Shotcut/config/Meltytech/Shotcut.conf",
      "~/.config/Meltytech/Shotcut.conf"
    ]
  }
}